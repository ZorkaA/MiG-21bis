<?xml version="1.0"?>
<PropertyList>

    <path>damperswitch.ac</path>
	
	
	
	<!-- picks and translations -->

	<animation>
		<type>pick</type>
		<object-name>damperSwitch</object-name>
		<action>
			<button>0</button>
			<button>1</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>/fdm/jsbsim/fcs/damping-enable</property>
				<step>1</step>
				<min>1</min>
				<max>3</max>
				<wrap>true</wrap>
			</binding>
		</action>
	</animation>

	<animation>
		<object-name>damperSwitch</object-name>
		<type>rotate</type>
		<property>/fdm/jsbsim/fcs/damping-enable</property>
		<factor>60</factor>
		<offset-deg>-90</offset-deg>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>



</PropertyList>