<?xml version="1.0"?>

<PropertyList>
      <path>miscpanel8.ac</path>

    <animation>
        <type>knob</type>
        <object-name>rwrPowerSwitch</object-name>
        <property>/fdm/jsbsim/electric/switches/rvfsp/rwr-ill-warn-set</property>
        <factor>-50</factor>
        <offset-deg>25</offset-deg>
        <center>
            <x-m>-0.006847</x-m>
            <y-m> 0.029197</y-m>
            <z-m> 0.017343</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>

        <action>
            <binding>
                <command>property-toggle</command>
                <property>/fdm/jsbsim/electric/switches/rvfsp/rwr-ill-warn-set</property>
            </binding>
        </action>
    </animation>

</PropertyList>
