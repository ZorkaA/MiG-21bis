    <animation>
        <type>textranslate</type>
        <object-name>chaffLeft10</object-name>
        <property>/fdm/jsbsim/systems/armament/aso2m/chaff-left</property>
        <bias>0.0001</bias>
        <factor>0.01</factor>
        <step>10</step>
        <axis>
            <x>0</x>
            <y>-1</y>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>chaffLeft1</object-name>
        <property>/fdm/jsbsim/systems/armament/aso2m/chaff-left</property>
