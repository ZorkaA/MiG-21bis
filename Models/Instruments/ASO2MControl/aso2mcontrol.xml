<!-- lights -->

    <animation>
        <type>material</type>
        <object-name>powerLight</object-name>
        <ambient>
            <red>0.0</red>
            <green>0.1</green>
            <blue>0.05</blue>
        </ambient>
        <emission>
            <red>0.0</red>
            <green>1.0</green>
            <blue>0.2</blue>
            <factor-prop>/fdm/jsbsim/systems/aso2m/lights/power</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>material</type>
        <object-name>armLight</object-name>
        <ambient>
            <red>0.0</red>
            <green>0.1</green>
            <blue>0.05</blue>
        </ambient>
        <emission>
            <red>0.0</red>
            <green>1.0</green>
            <blue>0.2</blue>
            <factor-prop>/fdm/jsbsim/systems/aso2m/lights/arm</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>material</type>
        <object-name>4gAutoLight</object-name>
        <ambient>
            <red>0.0</red>
            <green>0.1</green>
            <blue>0.05</blue>
        </ambient>
        <emission>
            <red>0.0</red>
            <green>1.0</green>
            <blue>0.2</blue>
            <factor-prop>/fdm/jsbsim/systems/aso2m/lights/AutoLight</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>material</type>
        <object-name>releaseLight</object-name>
        <ambient>
            <red>0.0</red>
            <green>0.1</green>
            <blue>0.05</blue>
        </ambient>
        <emission>
            <red>0.0</red>
            <green>1.0</green>
            <blue>0.2</blue>
            <factor-prop>/fdm/jsbsim/systems/aso2m/lights/releaseLight</factor-prop>
        </emission>
    </animation>

</PropertyList>