<?xml version="1.0"?>
<PropertyList>

    <path>countermeasurepanel.ac</path>

    <!--lights-->
    
    <animation>
        <type>material</type>
        <object-name>labels</object-name>
        <object-name>flareLeft10</object-name>
        <object-name>flareLeft1</object-name>
        <object-name>flareRight10</object-name>
        <object-name>flareRight1</object-name>
        <object-name>chaffLeft10</object-name>
        <object-name>chaffLeft1</object-name>
        <object-name>chaffRight10</object-name>
        <object-name>chaffRight1</object-name>
        <emission>
             <red>0.6</red>
             <green>0.3</green>
             <blue>0.3</blue>
            <factor-prop>controls/lighting/instruments-norm</factor-prop>
        </emission> 
    </animation>
    
    
    <!-- picks and translations -->

    <animation>
        <type>knob</type>
        <object-name>powerSwitch</object-name>
        <property>/fdm/jsbsim/electric/switches/cc/aso2m/power</property>
        <factor>-50</factor>
        <offset-deg>25</offset-deg>
        <center>
            <x-m>-0.010276</x-m>
            <y-m>-0.075939</y-m>
            <z-m>-0.007877</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>

        <action>
            <binding>
                <command>property-toggle</command>
                <property>/fdm/jsbsim/electric/switches/cc/aso2m/power</property>
            </binding>
        </action>
    </animation>

    <animation>
        <type>knob</type>
        <object-name>armSwitch</object-name>
        <property>/fdm/jsbsim/electric/switches/cc/aso2m/arm</property>
        <factor>-50</factor>
        <offset-deg>25</offset-deg>
        <center>
            <x-m>-0.010276</x-m>
            <y-m>-0.045939</y-m>
            <z-m>-0.007877</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>

        <action>
            <binding>
                <command>property-toggle</command>
                <property>/fdm/jsbsim/electric/switches/cc/aso2m/arm</property>
            </binding>
        </action>
    </animation>

    <animation>
        <type>knob</type>
        <object-name>4gAutoSwitch</object-name>
        <property>/fdm/jsbsim/electric/switches/cc/aso2m/auto4g</property>
        <factor>-50</factor>
        <offset-deg>25</offset-deg>
        <center>
            <x-m>-0.010276</x-m>
            <y-m>-0.015939</y-m>
            <z-m>-0.007877</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>

        <action>
            <binding>
                <command>property-toggle</command>
                <property>/fdm/jsbsim/electric/switches/cc/aso2m/auto4g</property>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>emergencyReleaseButton</object-name>
        <action>
            <button>0</button>
            <button>1</button>
            <button>2</button>
            <button>3</button>
            <button>4</button>
            <button>5</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-toggle</command>
                <property>/controls/armament/cm-trigger</property>
            </binding>
            <mod-up>
                <binding>
                    <command>property-toggle</command>
                    <property>/controls/armament/cm-trigger</property>
                </binding>
            </mod-up>
        </action>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>emergencyReleaseButton</object-name>
        <property></property>
        <factor>-0.0033</factor>
        <offset-m>0</offset-m>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>counterResetButton</object-name>
        <action>
            <button>0</button>
            <button>1</button>
            <button>2</button>
            <button>3</button>
            <button>4</button>
            <button>5</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-toggle</command>
                <property>/fdm/jsbsim/systems/armament/aso2m/reset</property>
            </binding>
            <binding>
                <command>property-assign</command>
                <property>/fdm/jsbsim/systems/armament/aso2m/flares-right</property>
                <value>20</value>
            </binding>
            <binding>
                <command>property-assign</command>
                <property>/fdm/jsbsim/systems/armament/aso2m/flares-left</property>
                <value>20</value>
            </binding>
            <binding>
                <command>property-assign</command>
                <property>/fdm/jsbsim/systems/armament/aso2m/chaff-right</property>
                <value>9</value>
            </binding>
            <binding>
                <command>property-assign</command>
                <property>/fdm/jsbsim/systems/armament/aso2m/chaff-left</property>
                <value>9</value>
            </binding>
            <mod-up>
                <binding>
                    <command>property-toggle</command>
                    <property>/fdm/jsbsim/systems/armament/aso2m/reset</property>
                </binding>
            </mod-up>
        </action>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>counterResetButton</object-name>
        <property>/fdm/jsbsim/systems/armament/aso2m/reset</property>
        <factor>-0.0023</factor>
        <offset-m>0</offset-m>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>flareReleaseButton</object-name>
        <action>
            <button>0</button>
            <button>1</button>
            <button>2</button>
            <button>3</button>
            <button>4</button>
            <button>5</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-toggle</command>
                <property>/fdm/jsbsim/systems/armament/aso2m/flare-trigger</property>
            </binding>
            <mod-up>
                <binding>
                    <command>property-toggle</command>
                    <property>/fdm/jsbsim/systems/armament/aso2m/flare-trigger</property>
                </binding>
            </mod-up>
        </action>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>flareReleaseButton</object-name>
        <property></property>
        <factor>-0.0033</factor>
        <offset-m>0</offset-m>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>chaffReleaseButton</object-name>
        <action>
            <button>0</button>
            <button>1</button>
            <button>2</button>
            <button>3</button>
            <button>4</button>
            <button>5</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-toggle</command>
                <property>/fdm/jsbsim/systems/armament/aso2m/chaff-trigger</property>
            </binding>
            <mod-up>
                <binding>
                    <command>property-toggle</command>
                    <property>/fdm/jsbsim/systems/armament/aso2m/chaff-trigger</property>
                </binding>
            </mod-up>
        </action>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>chaffReleaseButton</object-name>
        <property></property>
        <factor>-0.0033</factor>
        <offset-m>0</offset-m>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

<!-- textranslates -->

    <animation>
        <type>textranslate</type>
        <object-name>flareLeft10</object-name>
        <property>/fdm/jsbsim/systems/armament/aso2m/flares-left</property>
        <bias>0.0001</bias>
        <factor>0.01</factor>
        <step>10</step>
        <axis>
            <x>0</x>
            <y>-1</y>
        </axis>
    </animation>
    <animation>
        <type>textranslate</type>
        <object-name>flareLeft1</object-name>
        <property>/fdm/jsbsim/systems/armament/aso2m/flares-left</property>
        <bias>0.0001</bias>
        <factor>0.1</factor>
        <step>1</step>
        <axis>
            <x>0</x>
            <y>-1</y>
        </axis>
