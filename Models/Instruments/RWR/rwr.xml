<?xml version="1.0"?>
<PropertyList>

    <path>rwr.ac</path>

    <!--lights-->

    <!--yellow-->

    <animation>
        <type>material</type>
        <object-name>rwrYellowUL</object-name>
        <condition>
            <and>
                <or>
                    <equals>
                        <property>/instrumentation/rwr/forward-left/light-enable</property>
                        <value>1</value>
                    </equals>
                    <equals>
                        <property>/instrumentation/rwr/self-test</property>
                        <value>1</value>
                    </equals>
                </or>
                <equals>
                    <property>/instrumentation/rwr/red-yellow</property>
                    <value>0</value>
                </equals>
            </and>
        </condition>
        <emission>
            <red>0.8</red>
            <green>0.6</green>
            <blue>0.1</blue>
        </emission>
    </animation>

