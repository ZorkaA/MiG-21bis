    <!--test button-->

    <animation>
        <type>pick</type>
        <object-name>rwrTestButton</object-name>
        <condition>
            <greater-than>
                <property>/fdm/jsbsim/electric/output/rwr-ill-warn-set</property>
                <value>105</value>
            </greater-than>
        </condition>
        <action>
            <button>0</button>
            <button>1</button>
            <repeatable>false</repeatable>
            <interval-sec>0.2</interval-sec>
            <binding>
                <command>property-assign</command>
                <property>/instrumentation/rwr/self-test</property>
                <value>1</value>
            </binding>
            <mod-up>
            <binding>
                <command>property-assign</command>
                <property>/instrumentation/rwr/self-test</property>
                <value>0</value>
            </binding>
            </mod-up>
        </action>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>rwrTestButton</object-name>
        <property>/instrumentation/rwr/self-test</property>
        <factor>0.001</factor>
        <offset-m>0</offset-m>
        <axis>
            <x>-1.0</x>
            <y>0.00</y>
            <z>0.00</z>
        </axis>
    </animation>


    <animation>
        <type>rotate</type>
        <object-name>rwrKnob</object-name>
        <object-name>rwrRedLL</object-name>
        <object-name>rwrRedLR</object-name>
        <object-name>rwrRedUL</object-name>
        <object-name>rwrRedUR</object-name>
        <object-name>rwrYellowLL</object-name>
        <object-name>rwrYellowLR</object-name>
        <object-name>rwrYellowUL</object-name>
        <object-name>rwrYellowUR</object-name>
        <property>/instrumentation/rwr/red-yellow</property>
        <factor>-27.5</factor>
        <center>
            <x-m>0.016032</x-m>
            <y-m>0.004834</y-m>
            <z-m>0.002548</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>rwrKnob</object-name>
        <object-name>rwrRedLL</object-name>
        <object-name>rwrRedLR</object-name>
        <object-name>rwrRedUL</object-name>
        <object-name>rwrRedUR</object-name>
        <object-name>rwrYellowLL</object-name>
        <object-name>rwrYellowLR</object-name>
        <object-name>rwrYellowUL</object-name>
        <object-name>rwrYellowUR</object-name>
        <action>
            <button>0</button>
            <button>1</button>
            <button>2</button>
            <button>3</button>
            <button>4</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-toggle</command>
                <property>/instrumentation/rwr/red-yellow</property>
            </binding>
        </action>
    </animation>  

    <animation>
        <type>knob</type>
        <object-name>rwrVolume</object-name>
        <property>/instrumentation/rwr/volume</property>
        <factor>280</factor>
        <offset-deg>0</offset-deg>
        <center>
            <x-m> 0.001215</x-m>
            <y-m>-0.053156</y-m>
            <z-m>-0.028030</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>

        <action>
            <binding>
                <command>property-adjust</command>
                <property>/instrumentation/rwr/volume</property>
                <factor>0.05</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    
    
</PropertyList>