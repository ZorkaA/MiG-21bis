        </switch>
        <switch name="/instrumentation/adf/power-btn">
            <default value="0"/>
            <test logic="AND" value="1">
                electric/output/arc gt 107
            </test>
        </switch>
        <switch name="/instrumentation/nav/power-btn">
            <default value="0"/>
            <test logic="AND" value="1">
                electric/output/rsbn gt 107
            </test>
        </switch>
    </channel>

